matlab
a0=input('Введите значение a0: 1');
a1=input('Введите значение a1: 20');
a2=input('Введите значение a2: -9');
a3=input('Введите значение a3: 2');
a4=input('Введите значение a4: 1');
x=-5:0.5:5;
a=[a0 a1 a2 a3 a4];
y=a0+a1*x+a2*x.^2+a3*x.^3+a4*x.^4;
fmax=max(y);
fmin=min(y);
plot(x,y); 
grid on;
xlabel('x'); 
ylabel('y');
