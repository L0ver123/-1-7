def f(x):
    return (x**4) + (2*(x**3)) - (9*(x**2)) - (20*x) + 1

# Ищем наибольшее и наименьшее значение функции на отрезке -5;5
max_val = f(-5)
min_val = f(-5)
for i in range(-4, 6):
    if f(i) > max_val:
        max_val = f(i)
    if f(i) < min_val:
        min_val = f(i)

# Выводим наибольшее и наименьшее значение функции
print("Наибольшее значение функции: ", max_val)
print("Наименьшее значение функции: ", min_val)
