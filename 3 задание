x = float(input("Введите значение x: "))
y = float(input("Введите значение y: "))
min_value = min(x, y)
max_value = max(x, y)
z = (min_value + 0.2) / (max_value + min_value)
print(f"Значение x: {x}")
print(f"Значение y: {y}")
print(f"Минимум из x и y: {min_value}")
print(f"Максимум из x и y: {max_value}")
print(f"Значение z: {z}")
