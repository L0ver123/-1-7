import math
x1, y1 = 2, 3
x2 = 3
x3, y3 = 2, 1
x4 = 0.5

r1 = x1**((y1**2) + 4) - math.exp(-3*x1)
print(f"Результат первого выражения: {r1}")

r2 = 2*(math.sin(x2/2))**2 - (math.sin(x2**(1/2)))**3
print(f"Результат второго выражения: {r2}")

r3 = (((math.log(abs(x3))**3) + 4*x3*y3) / (5*x3*y3))
print(f"Результат третьего выражения: {r3}")

r4 = ((math.acos(x4) - 7*math.log(x4**2)) / (x4 + 7.3))
print(f"Результат четвертого выражения: {r4}")
